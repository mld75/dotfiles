(defun j0xaf/setup-mu4e ()
  (setq mu4e-archive-folder "/Gmail/Archive")
  (setq mu4e-sent-folder "/Gmail/Sent")
  (setq mu4e-trash-folder "/Gmail/Trash")
  (setq mu4e-drafts-folder "/Gmail/Drafts")
  (setq mu4e-get-mail-command "mbsync -a")

  ;; don't save message to Sent Messages, Gmail/IMAP takes care of this
  (setq mu4e-sent-messages-behavior 'delete)
  (setq mu4e-change-filenames-when-moving t) ;; for mbsync

  (add-hook 'mu4e-view-mode-hook #'visual-fill-column-mode)
  (setq shr-use-fonts nil)
  (setq mu4e-attachment-dir  "~/Downloads")
  (setq mu4e-view-show-addresses 't)
  (setq mu4e-confirm-quit nil)
  (setq mu4e-refile-folder "/Gmail/Archive")
  (setq   mu4e-maildir-shortcuts
          '((:maildir "/Gmail/Archive" :key ?a)
            (:maildir "/Gmail/Inbox"   :key ?i)
            (:maildir "/Gmail/Starred"    :key ?s)
            (:maildir "/Gmail/Sent"    :key ?t)))
  (setq mu4e-compose-complete-only-personal 't)
  (setq mu4e-bookmarks
        '(( :name  "Unread messages in inbox"
                   :query "flag:unread AND m:/Gmail/Inbox AND NOT (m:/Gmail/Trash OR flag:trashed)"
                   :key ?u)
          ( :name  "Other unread messages"
                   :query "flag:unread AND NOT (m:/Gmail/Inbox OR m:/Gmail/Trash OR flag:trashed)"
                   :key ?U)
          ( :name "Today's messages"
                  :query "date:today..now AND NOT flag:trashed"
                  :key ?t)
          ( :name "Last 7 days"
                  :query "date:7d..now AND NOT flag:trashed"
                  :hide-unread t
                  :key ?w)
          ( :name "Messages with images"
                  :query "mime:image/* AND NOT flag:trashed"
                  :key ?p)
          ( :name  "Big messages"
                   :query "size:5M..500M"
                   :key   ?b)))
  )

;; Do not write anything past this comment. This is where Emacs will
;; auto-generate custom variable definitions.
(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(elm-compile-command "/usr/local/bin/elm-make")
 '(evil-want-Y-yank-to-eol t)
